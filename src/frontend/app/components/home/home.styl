#artist-table
  text-align center
  th
    text-align center

  td
    vertical-align middle !important

  img
    height 100px

  progress
    // Negative value is used to cause progress-value to overlap progressbar
    margin-bottom -1.28rem

  .progress-value
    color rgba(0, 0, 0, 0.72)
    font-weight bold
    font-size 0.9rem

